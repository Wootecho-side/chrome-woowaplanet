{
  "manifest_version": 3,
  "name": "WoowaPlanet",
  "version": "1.1",
  "description": "우아한 테크코스의 마스코트 행성이 크롬 확장 프로그램입니다!",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
